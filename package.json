{
  "name": "soit",
  "version": "1.1.0",
  "description": "Create a type guard from a list of literals.",
  "main": "dist/index.js",
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/benchmark": "^2.1.1",
    "benchmark": "^2.1.4",
    "jest": "^26.4.2",
    "marked": "^0.4.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-expect": "^1.3.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.4.1",
    "typescript": "^4.0.3",
    "zod": "^1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "test": "jest --coverage && npm run compile",
    "compile": "tsc -p ./tsconfig.test.json --noEmit",
    "prettier": "prettier '(src|tests|benchmark)/**/*.ts' --write",
    "benchmark": "ts-node --project tsconfig.test.json benchmark/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrgautier/soit.git"
  },
  "keywords": [
    "utils",
    "typescript",
    "guard",
    "literal",
    "union"
  ],
  "author": "Adrien Gautier",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/tests/.*.test.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "dot-prop": "^6.0.1"
  }
}
