{
  "name": "soit",
  "version": "1.2.0",
  "description": "Create a type guard from a list of literals.",
  "main": "dist/index.js",
  "sideEffects": false,
  "devDependencies": {
    "@types/benchmark": "2.1.4",
    "@types/jest": "29.5.7",
    "@types/node": "20.8.10",
    "benchmark": "2.1.4",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "ts-expect": "1.3.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "zod": "1.11.17"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "test": "jest --coverage && npm run compile",
    "compile": "tsc -p ./tsconfig.test.json --noEmit",
    "prettier": "prettier '(src|tests|benchmark)/**/*.ts' --write",
    "benchmark": "ts-node --project tsconfig.test.json benchmark/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrgautier/soit.git"
  },
  "keywords": [
    "utils",
    "typescript",
    "guard",
    "literal",
    "union"
  ],
  "author": "Adrien Gautier",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/tests/.*.test.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "dot-prop": "6.0.1"
  }
}
